    "--  3 QUESTION\n",
    "\n",
    "-- Calculate the total and average salary for active employees.\n",
    "\n",
    "SELECT \n",
    "    COUNT(*) AS total_employees,\n",
    "    SUM(salary) AS total_salary,\n",
    "    AVG(salary) AS avg_salary\n",
    "FROM \n",
    "    employees\n",
    "WHERE \n",
    "    is_active = TRUE;\n"
   ]
  }
