    "-- QUESTION\n",
    "\n",
    "-- Find employees who checked in late (after shift_start by more than 10 minutes)\n",
    "\n",
    "SELECT emp_id, emp_name, check_in, shift_start\n",
    "FROM employee_attendance\n",
    "WHERE check_in::time > (shift_start + INTERVAL '10 minutes');\n",
    "\n",
    "\n"
