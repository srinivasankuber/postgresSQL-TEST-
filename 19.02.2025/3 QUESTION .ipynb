    "-- 3 QUESTION\n",
    "\n",
    "-- Increase the price of all Apple products by 10%.\n",
    "\n",
    "UPDATE products_catalog\n",
    "SET details = jsonb_set(\n",
    "    details, \n",
    "    '{price}', \n",
    "    to_jsonb((details->>'price')::NUMERIC * 1.10)\n",
    ")\n",
    "WHERE details->>'brand' = 'Apple';\n"
   ]
  }
