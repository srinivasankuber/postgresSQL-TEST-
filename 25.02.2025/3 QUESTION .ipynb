    "-- 3 QUESTION\n",
    "\n",
    "-- Find the total number of products in each category and the average price of products in that category.\n",
    "\n",
    "SELECT \n",
    "    category,\n",
    "    COUNT(*) AS total_products,\n",
    "    AVG(price) AS avg_price\n",
    "FROM products\n",
    "GROUP BY category\n",
    "ORDER BY category;\n",
    "\n",
    "\n"
   ]
