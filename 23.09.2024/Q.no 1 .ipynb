   "source": [
    "-- 1  list all authors and numbers of book wriiten ?\n",
    "\n",
    "SELECT \n",
    "    a.\"AuthorName\", \n",
    "    COUNT(b.\"BookID\") AS \"NumberOfBooks\"\n",
    "FROM \n",
    "    authors_table a\n",
    "LEFT JOIN \n",
    "    book_table b \n",
    "ON \n",
    "    a.\"AuthorID\" = b.\"AuthorID\"\n",
    "GROUP BY \n",
    "    a.\"AuthorName\"\n",
    "ORDER BY \n",
    "    \"AuthorName\";"
   ]
  }
