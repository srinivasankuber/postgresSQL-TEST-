   "source": [
    "--2 find the average temperature for each day\n",
    "\n",
    "\n",
    "SELECT \n",
    "    DATE(\"reading_time\") AS date,\n",
    "    ROUND(AVG(temperature::numeric), 2) AS average_temperature\n",
    "FROM \n",
    "    temperature\n",
    "GROUP BY \n",
    "    DATE(\"reading_time\")\n",
    "ORDER BY \n",
    "    DATE(\"reading_time\");"
   ]
  }
