    "--1 find the products that have not been orderd at all\n",
    "\n",
    "\n",
    "SELECT  p.productname\n",
    "FROM products p\n",
    "LEFT JOIN orders o ON p.productid = o.productid\n",
    "WHERE o.orderid IS NULL;\n",
    "\n"
   ]
