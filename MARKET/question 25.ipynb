    "-- 25  Retrieve the order count and total amount for each status.\n",
    "\n",
    "select\n",
    "       count(order_id) AS order_count,\n",
    "       sum(total_amount) AS total_amount,\n",
    "\t   status\n",
    "from orders\n",
    "group by status;\n"
